{
    "version": 2,
    "builds": [
        {
            "src": "homepage/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/homepage/$1"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}